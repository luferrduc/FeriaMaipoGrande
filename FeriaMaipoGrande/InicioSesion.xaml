<Window x:Class="FeriaMaipoGrande.InicioSesion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FeriaMaipoGrande"
        mc:Ignorable="d"
        Title="InicioSesion" Height="450" Width="800">
    <Grid Margin="0,0,0,-6">
        <Button x:Name="btnIniciar" Content="Iniciar Sesión" HorizontalAlignment="Left" Margin="389,289,0,0" VerticalAlignment="Top" Width="96" Click="btnIniciar_Click"/>
        <TextBox x:Name="txUsuario" HorizontalAlignment="Left" Margin="365,185,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txPassword" HorizontalAlignment="Left" Margin="365,218,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Usuario: " HorizontalAlignment="Left" Margin="304,181,0,0" VerticalAlignment="Top"/>
        <Label Content="Contraseña: " HorizontalAlignment="Left" Margin="284,214,0,0" VerticalAlignment="Top"/>
        <DataGrid x:Name="dgNombres" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="30,59,375,286" Visibility="Hidden"/>


    </Grid>
</Window>
